package com.generalframework;

public class Individual {
	
	double 	OBJECTIVE_VALUE;
	double FITNESS;
	int[] CHROMOSOME;
	int[] XBIN;
	int[] LOWER_BOUND;
	int[] UPPER_BOUND;
	
	public Individual(int length,int n) {
		CHROMOSOME = new int[length];
		XBIN = new int[n];
		LOWER_BOUND = new int[n];
	}
}
